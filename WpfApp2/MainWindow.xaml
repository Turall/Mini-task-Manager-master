<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="650" Background="DarkKhaki"  ResizeMode="NoResize"  >
    <Grid Width="644">
        <ListBox Name="processList" HorizontalAlignment="Left" Height="409" VerticalAlignment="Top" Width="289"/>
        <TextBox Name="Txtbox" HorizontalAlignment="Left" Height="29" Margin="448,47,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="131"/>
        <TextBlock HorizontalAlignment="Left" Margin="342,52,0,0" TextWrapping="Wrap" Text="Process Name" VerticalAlignment="Top" RenderTransformOrigin="0.466,0.592" Height="22" Width="76"/>
        <Button Name="StartBtn" Content="Start" Foreground="Green" HorizontalAlignment="Left" Margin="328,143,0,0" VerticalAlignment="Top" Width="75" Click="StartBtn_Click"/>
        <Button Name="KillBtn" Content="Kill" Foreground="Red" HorizontalAlignment="Left" Margin="493,143,0,0" VerticalAlignment="Top" Width="75" Click="KillBtn_Click"/>

    </Grid>
</Window>
